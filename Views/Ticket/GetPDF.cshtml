@model CorePdf.Models.TicketModel

@{
    ViewData["Title"] = "GetPDF";
}

 
<form asp-action="MVCTOPDF">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input type="hidden" asp-for="GUID" />

    <div class="form-group">
            <label asp-for="Name" class="control-label"></label>
            <p>@Model.Name</p>
    </div>
    <div class="form-group">
        <label asp-for="LastName" class="control-label"></label>
        <p>@Model.LastName</p>
    </div>
    <div class="form-group">
        <label asp-for="SeatNo" class="control-label"></label>
        <p>@Model.SeatNo</p>
    </div>
        
        
    <div class="form-group">
       
        <button name="submitbutton" value="PDF Confirmed Rotativa" class="btn btn-primary">Rotativa</button>
        <button name="submitbutton" value="OpenHtmlToPdf" class="btn btn-primary">OpenHtmlToPdf</button>
        <button name="submitbutton" value="HtmlToPdf" class="btn btn-primary">HtmlToPdf</button>


    </div>
</form>

